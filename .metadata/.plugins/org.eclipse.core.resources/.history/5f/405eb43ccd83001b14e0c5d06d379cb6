import java.util.Scanner;
import java.io.*;


public class SkeeBall {
	
	static final int MAX_PLAYS = 8;
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		System.out.print("Enter the number of plays (1-8) ");
		int n = in.nextInt();
		
		switch(n) {
			case 1:
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			case 1:
				break;
			case 2:
				break;
			case 3:
				break;
			case 4:
				break;
			default:
				break;
			
		}
	}
}
